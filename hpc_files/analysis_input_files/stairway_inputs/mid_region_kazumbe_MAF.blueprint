#input setting
popid: mid_region_kazumbe_MAF # id of the population (no white space)
nseq: 16 # number of sequences
L: 3101158 # total number of observed nucleic sites, including polymorphic and monomorphic
whether_folded: true # whether the SFS is folded (true or false)
SFS:    13002.0659247975 7014.82856944108 4780.85817274794 3638.00941469455 3071.72498652633 2808.09905242383 2700.20195169559 1336.19209763714 # snp frequency spectrum: number of singleton, number of doubleton, etc. (separated by white space)
