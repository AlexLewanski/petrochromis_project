stairway_plot_dir: /project/ltcichlidgenomics/alewansk/Petrochromis_FULLDATASET/demographic_mod/stairway_plot/stairway_plot_v2.1.1/stairway_plot_es # directory to the stairway plot files
ninput: 1000 # number of input files to be created for each estimation
#random_seed: NONE
#output setting
mu: 3.5e-9 # assumed mutation rate per site per generation (.e.g 1.2e-8)
year_per_generation: 2 # assumed generation time (in years)
